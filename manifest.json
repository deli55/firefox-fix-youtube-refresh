{
  "manifest_version": 3,
  "name": "YouTube Bug Fixer",
  "version": "1.0",
  "description": "Fixes a specific bug on YouTube.",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
